{
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.21",
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.11.9",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.3"
  },
  "scripts": {
    "dev": "concurrently \"yarn watch-compile\" \"yarn watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "build": "swc src -d dist",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "commander": "^9.4.1",
    "inquirer": "^8.0.0"
  },
  "name": "ts-template",
  "version": "1.0.0",
  "main": "index.js",
  "author": "suparthghimire <suparthnarayanghimire2014@gmail.com>",
  "license": "MIT"
}
